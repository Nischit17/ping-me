{
  "expo": {
    "name": "PingMe",
    "slug": "pingme-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pingme.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pingme.app"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ]
    ],
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "dc1909cd-acf5-44a6-9235-dce3fa23c6e0"
      }
    },
    "owner": "nischit05",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/dc1909cd-acf5-44a6-9235-dce3fa23c6e0"
    }
  }
